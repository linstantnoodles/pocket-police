{
  "manifest_version": 2,
  "name": "Pocket Police",
  "version": "0.3",
  "web_accessible_resources": [
      "prison-cell-bars.png",
      "arrest-btn.png",
      "arrest-btn-2.png",
      "release-btn.png"
   ],
  "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js": [
          "jquery-3.2.1.min.js",
          "api-client.js",
          "element-watcher.js",
          "element-tagger.js",
          "button.js",
          "content.js"
        ]
    }
  ],
  "browser_action": {
    "default_icon": "icon-2.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}