{
  "manifest_version": 2,
  "name": "Pocket Police",
  "version": "0.1",
  "web_accessible_resources": [
      "prison-cell-bars.png"
   ],
  "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js": [
          "jquery-3.2.1.min.js",
          "api-client.js",
          "element-tagger.js",
          "button.js",
          "content.js"
        ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}